    "tailwindcss": "^3.3.3",
    "trix": "^2.0.0-alpha.1"
  },
  "scripts": {
    "build:css": "tailwindcss --minify --postcss ./config/postcss.config.js -c ./config/tailwind.config.js -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css",
